{
  "name": "boredbanana",
  "version": "1.0.0",
  "description": "banana timer",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js | tap-spec",
    "start": "nodemon ./server.js",
    "build": "browserify -t hbsfy ./src/index.js -g uglifyify > ./client/app.js",
    "serve": "node-dev server.js",
    "watch:index": "watchify -t hbsfy ./src/index.js -o ./client/app.js -dv",
    "watch:time": "watchify ./src/timer.js -o client/apptimer.js -v",
    "watch": "npm-run-all --parallel watch:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxine-ellah/personal-project.git"
  },
  "keywords": [
    "banana"
  ],
  "author": "maxine-ellah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxine-ellah/personal-project/issues"
  },
  "homepage": "https://github.com/maxine-ellah/personal-project#readme",
  "dependencies": {
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "hbs": "^4.0.0",
    "knex": "^0.11.7",
    "moment-timezone": "^0.5.3",
    "node-dev": "^3.1.0",
    "nodemon": "^1.9.1",
    "pg": "^5.1.0",
    "superagent": "^1.8.3"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "cheerio": "^0.20.0",
    "fs": "0.0.2",
    "handlebars": "^4.0.5",
    "hbsfy": "^2.6.0",
    "jquery": "^2.2.3",
    "moment": "^2.12.0",
    "npm-run-all": "^1.7.0",
    "supertest": "^1.2.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
